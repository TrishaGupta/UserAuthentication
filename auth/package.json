{
  "name": "authentication-api-starter-code",
  "version": "1.0.0",
  "description": "Starter code of the tutorial for building a password Authentication API.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-backend\" \"npm run frontend-start\"",
    "dev-server": "webpack serve --config webpack.dev.js",
    "dev-backend": "nodemon app.js --host localhost --port 8080",
    "frontend-start": "cd frontend && npm start"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.3.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "1.18.4"
  },
  "keywords": [
    "authentication",
    "express",
    "jwt"
  ]
}
